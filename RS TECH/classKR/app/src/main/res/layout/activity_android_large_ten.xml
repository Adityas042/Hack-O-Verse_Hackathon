<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLargeTenVM" type="com.classkr.app.modules.androidlargeten.data.viewmodel.AndroidLargeTenVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearAndroidLarge"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackvecteezytechno"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageVecteezytechno"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_vecteezytechno}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vecteezytechno"/>
            <LinearLayout android:id="@+id/linearColumnrectanglesixteen"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackrectanglesixteen"
                    android:layout_height="@dimen/_141pxv" android:layout_width="match_parent">
                    <View android:background="@color/indigo_A700"
                        android:id="@+id/viewRectangleSixteen"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_65pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_65pxh"/>
                    <androidx.appcompat.widget.Toolbar
                        android:id="@+id/toolbarToolbar"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        app:contentInsetStart="0dp" style="@style/groupStylelight_blue_100">
                        <LinearLayout android:id="@+id/linearRowmenu"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageMenu"
                                android:layout_height="@dimen/_30pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="@dimen/_33pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_menu"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                            <LinearLayout
                                android:id="@+id/linearRowsearch"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_197pxh"
                                android:layout_weight="0.74"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageSearch"
                                    android:layout_height="@dimen/_39pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_width="@dimen/_39pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_search"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                                <ImageView android:id="@+id/imageUser"
                                    android:layout_height="@dimen/_41pxh"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:layout_width="@dimen/_37pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_user"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                            </LinearLayout>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnStudentFeedbackOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_25pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_25pxv"
                        android:singleLine="true"
                        android:text="@string/msg_student_feedbac"
                        android:textAllCaps="false"
                        style="@style/btnSolid_1" tools:text="@string/msg_student_feedbac"/>
                </FrameLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnleaveanote"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleyellow_A400cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLeaveaNote"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_27pxv"
                        android:layout_marginEnd="@dimen/_47pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_marginTop="@dimen/_26pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{androidLargeTenVM.androidLargeTenModel.txtLeaveaNote}"
                        style="@style/txtInterbold40_1" tools:text="@string/lbl_leave_a_note"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnaddfeedback"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecyan_200cornerRadius">

                    <EditText
                        android:id="@+id/txtAddFeedback"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_303pxv"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:singleLine="true"
                        android:text="@{androidLargeTenVM.androidLargeTenModel.txtAddFeedback}" />
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSelectTheRecepientToSend"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@string/msg_select_the_rece"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/msg_select_the_rece"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSubmit"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_37pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_123pxh"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_submit"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_submit"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
