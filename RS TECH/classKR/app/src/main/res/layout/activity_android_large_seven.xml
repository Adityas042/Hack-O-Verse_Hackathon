<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLargeSevenVM" type="com.classkr.app.modules.androidlargeseven.data.viewmodel.AndroidLargeSevenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackvecteezytechno"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageVecteezytechno"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_767pxv"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_vecteezytechno}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vecteezytechno"/>
        <LinearLayout android:id="@+id/linearColumnrectanglesixteen"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackrectanglesixteen"
                android:layout_height="@dimen/_66pxv" android:layout_width="match_parent">
                <View android:background="@color/indigo_A700"
                    android:id="@+id/viewRectangleSixteen"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_65pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_65pxh"/>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    app:contentInsetStart="0dp" style="@style/groupStylelight_blue_100">
                    <LinearLayout android:id="@+id/linearRowmenu"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                        <LinearLayout android:id="@+id/linearRowsearch"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_197pxh"
                            android:layout_weight="0.74"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_39pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_width="@dimen/_39pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                            <ImageView android:id="@+id/imageUser"
                                android:layout_height="@dimen/_41pxh"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_width="@dimen/_37pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </FrameLayout>
            <ScrollView android:clipToPadding="false"
                android:fillViewport="true"
                android:id="@+id/scrollViewScrollview"
                android:layout_height="match_parent"
                android:layout_weight="1" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameAndroidLarge"
                    android:layout_height="@dimen/_932pxv" android:layout_width="match_parent">
                    <View android:id="@+id/viewGroupFortyEight"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_66pxv"
                        android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_360pxh"/>
                    <FrameLayout
                        android:id="@+id/frameStackvecteezytechnoOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_800pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                        <ImageView
                            android:id="@+id/imageVecteezytechnoOne"
                            android:layout_gravity="start"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_vecteezytechno}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vecteezytechno"/>
                        <LinearLayout
                            android:id="@+id/linearColumnstudentCourseYear"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnStudentCourseYear"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_19pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_19pxv"
                                android:singleLine="true"
                                android:text="@string/msg_student_course"
                                android:textAllCaps="false"
                                style="@style/btnSolid_2" tools:text="@string/msg_student_course"/>
                            <LinearLayout
                                android:id="@+id/linearRowgroupfortytwo"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtGroupFortyTwo"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:layout_width="0dp"
                                    android:paddingBottom="@dimen/_19pxv"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_19pxv"
                                    android:singleLine="true"
                                    android:text="@{androidLargeSevenVM.androidLargeSevenModel.txtGroupFortyTwo}"
                                    style="@style/txtSolid_1" tools:text="@string/lbl_semester"/>

                                <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnSection"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_14pxh"
                                    android:layout_weight="0.5"
                                    android:layout_width="0dp"
                                    android:paddingBottom="@dimen/_19pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_19pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_section"
                                    android:textAllCaps="false"
                                    style="@style/btnSolid_2" tools:text="@string/lbl_section"/>
                            </LinearLayout>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnSubmit"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_118pxh"
                                android:layout_marginStart="@dimen/_118pxh"
                                android:layout_marginTop="@dimen/_26pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_8pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_8pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_submit"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_2" tools:text="@string/lbl_submit"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtResult"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_118pxh"
                                android:layout_marginStart="@dimen/_118pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{androidLargeSevenVM.androidLargeSevenModel.txtResult}"
                                style="@style/txtInterbold16" tools:text="@string/lbl_result"/>
                            <FrameLayout
                                android:id="@+id/frameStacklanguage"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_548pxv"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_359pxh">
                                <LinearLayout
                                    android:id="@+id/linearColumn"
                                    android:layout_gravity="top|start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStylelight_blue_100cornerRadius">
                                    <LinearLayout
                                    android:gravity="start"
                                    android:id="@+id/linearColumnlanguage"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_209pxv"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_19pxv"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStyleyellow_A400">
                                    <TextView
                                    android:id="@+id/txtLanguage"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_13pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{androidLargeSevenVM.androidLargeSevenModel.txtLanguage}"
                                    style="@style/txtInterbold16_1"
                                        tools:text="1. Student Name:                                                   Regd.No.:                                 ASSIGNMENT SUBMITTED"/>

                                    <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnPreviousResultsOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:paddingBottom="@dimen/_13pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_13pxv"
                                    android:singleLine="true"
                                    android:text="@string/msg_previous_result"
                                    android:textAllCaps="false"
                                    style="@style/btnSolid_5" tools:text="@string/msg_previous_result"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                    android:id="@+id/linearColumnOne"
                                    android:layout_gravity="bottom|start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStylelight_blue_100cornerRadius">
                                    <LinearLayout
                                    android:gravity="start"
                                    android:id="@+id/linearColumnlanguageOne"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_209pxv"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_19pxv"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStyleyellow_A400">
                                    <TextView
                                    android:id="@+id/txtLanguageOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_13pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{androidLargeSevenVM.androidLargeSevenModel.txtLanguageOne}"
                                    style="@style/txtInterbold16_1"
                                        tools:text="2. Student Name:                                                   Regd.No.:                                 ASSIGNMENT NOT SUBMITTED"/>

                                    <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnPreviousResultsThree"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:paddingBottom="@dimen/_13pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_13pxv"
                                    android:singleLine="true"
                                    android:text="@string/msg_previous_result"
                                    android:textAllCaps="false"
                                    style="@style/btnSolid_5" tools:text="@string/msg_previous_result"/>
                                    </LinearLayout>
                                </LinearLayout>
                            </FrameLayout>
                        </LinearLayout>
                    </FrameLayout>

                    <TextView
                        android:id="@+id/txtGroupFortyFour"
                        style="@style/txtSolid_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|start"
                        android:layout_marginTop="@dimen/_65pxv"
                        android:layout_marginBottom="@dimen/_65pxv"
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_30pxv"
                        android:paddingBottom="@dimen/_18pxv"
                        android:rotationY="1"
                        android:singleLine="true"
                        android:text="@{androidLargeSevenVM.androidLargeSevenModel.txtGroupFortyFour}"
                        tools:text="@string/msg_student_record" />
                </FrameLayout>
            </ScrollView>
        </LinearLayout>
    </FrameLayout>
</layout>
