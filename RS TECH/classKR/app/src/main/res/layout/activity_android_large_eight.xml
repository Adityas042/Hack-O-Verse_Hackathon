<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLargeEightVM" type="com.classkr.app.modules.androidlargeeight.data.viewmodel.AndroidLargeEightVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStack"
        android:layout_height="match_parent" android:layout_width="match_parent">

        <FrameLayout
            android:id="@+id/frameStackvecteezytechno"
            android:layout_width="match_parent"
            android:layout_height="907dp"
            android:layout_gravity="start">

            <ImageView
                android:id="@+id/imageVecteezytechno"
                imageRes="@{@drawable/img_vecteezytechno}"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_767pxv"
                android:layout_gravity="top|start"
                android:layout_marginBottom="@dimen/_10pxv"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_vecteezytechno" />

            <LinearLayout
                android:id="@+id/linearColumnrectanglesixteen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_10pxv"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/frameStackrectanglesixteen"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_141pxv">

                    <View
                        android:id="@+id/viewRectangleSixteen"
                        android:layout_width="@dimen/_65pxh"
                        android:layout_height="@dimen/_65pxh"
                        android:layout_gravity="top|start"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:background="@color/indigo_A700" />

                    <androidx.appcompat.widget.Toolbar
                        android:id="@+id/toolbarToolbar"
                        style="@style/groupStylelight_blue_100"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|start"
                        android:layout_marginBottom="@dimen/_10pxv"
                        app:contentInsetStart="0dp">

                        <LinearLayout
                            android:id="@+id/linearRowmenu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageMenu"
                                android:layout_width="@dimen/_33pxh"
                                android:layout_height="@dimen/_30pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_menu"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_menu" />

                            <LinearLayout
                                android:id="@+id/linearRowsearch"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_197pxh"
                                android:layout_weight="0.74"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/imageSearch"
                                    android:layout_width="@dimen/_39pxh"
                                    android:layout_height="@dimen/_39pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_search"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_search" />

                                <ImageView
                                    android:id="@+id/imageUser"
                                    android:layout_width="@dimen/_37pxh"
                                    android:layout_height="@dimen/_41pxh"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_user"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_user" />
                            </LinearLayout>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>

                    <LinearLayout
                        android:id="@+id/linearColumnstaffandfacul"
                        style="@style/groupStyleblack_900"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|start"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtStaffandFacul"
                            style="@style/txtInterbold20_1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_101pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_marginEnd="@dimen/_90pxh"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:gravity="center_horizontal"
                            android:text="@{androidLargeEightVM.androidLargeEightModel.txtStaffandFacul}"
                            tools:text="@string/msg_staff_and_facul" />
                    </LinearLayout>
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearAndroidLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearColumnstaffFacultyOne"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearRowstaffFacultyOne"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnStaffFacultyOne"
                                style="@style/btnSolid_6"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.54"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_10pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingBottom="@dimen/_10pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_staff_faculty"
                                android:textAllCaps="false"
                                tools:text="@string/lbl_staff_faculty" />

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnContact"
                                style="@style/btnSolid_6"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_weight="0.46"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_10pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingBottom="@dimen/_10pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_contact"
                                android:textAllCaps="false"
                                tools:text="@string/lbl_contact" />
                        </LinearLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerListnameone"
                            itemSpace="@{@dimen/_7pxh}"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            tools:itemCount="6"
                            tools:listitem="@layout/row_listnameone" />

                        <LinearLayout
                            android:id="@+id/linearRownameseven"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnNameSeven"
                                style="@style/btnSolid_7"
                                android:layout_width="237dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_25pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingBottom="@dimen/_25pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_name7"
                                android:textAllCaps="false"
                                tools:text="@string/lbl_name7" />

                            <ImageButton
                                android:id="@+id/btnCallSix"
                                android:layout_width="@dimen/_69pxh"
                                android:layout_height="@dimen/_69pxh"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:background="@drawable/rectangle_bg_light_green_a700_radius_34_5"
                                android:paddingHorizontal="@dimen/_15pxh"
                                android:paddingVertical="@dimen/_15pxv"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_call"
                                tools:ignore="ContentDescription" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </FrameLayout>

    </FrameLayout>
</layout>
